@model GetWhyChooseYummyByIdDto

@{
    ViewData["Title"] = "UpdateWhyChooseYummy";
    Layout = "~/Views/AdminLayout/Index.cshtml";
}

<section class="section">
    <div class="section-body">
        <div class="row">
            <div class="col-12 col-md-12 col-lg-12">
                <div class="card">
                    <div class="card-header">
                        <h4>Update - Why Should You Choose Us</h4>
                    </div>
                    <div class="card-body">
                        <form method="post">
                            <input type="hidden" asp-for="ServiceId" />

                            <label>Title</label>
                            <input type="text" asp-for="Title" class="form-control" />
                            <br />

                            <label>Description</label>
                            <input type="text" asp-for="Description" class="form-control" />
                            <br />

                            <label>Icon URL</label>
                            <input type="text" asp-for="IconUrl" class="form-control" />
                            <br />

                            <button class="btn btn-success">Update Field</button>
                        </form>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>
